{
  "name": "azadshaikh/s3phpcmd",
  "description": "PHP CLI tools to manage S3. S3 to S3 Migrations and Backup",
  "keywords": [
    "php",
    "S3",
    "CLI",
    "cmd",
    "migrations",
    "backups"
  ],
  "homepage": "https://github.com/azadshaikh/s3phpcmd",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Azad Shaikh",
      "email": "mohd.azad.shaikh@gmail.com",
      "homepage": "https://github.com/azadshaikh"
    }
  ],
  "require": {
    "php": ">=7.4",
    "league/flysystem": "^3.0",
    "league/flysystem-aws-s3-v3": "^3.0",
    "league/flysystem-ftp": "^3.0",
    "symfony/console": "6.1.x-dev",
    "symfony/dotenv": "^6.0"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Azadshaikh\\S3phpCmd\\": "src/"
    }
  },
  "minimum-stability": "stable"
}
